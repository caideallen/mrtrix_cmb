mrregister  wmfod2/10102_wmfod.mif -mask1 dwimask/10102_dwi_mask.mif template3/wmfod_template_vox_2.mif -nl_warp template3/maskintemp_2/10102_subject2template_warp.mif template3/maskintemp_2/10102_template2subject_warp.mif -force
mrtransform dwimask/10102_dwi_mask.mif -warp template3/maskintemp_2/10102_subject2template_warp.mif -interp nearest -datatype bit template3/maskintemp_2/10102_dwi_mask_in_template_space.mif -force

subjects=(
  10107
  11018
  11026
  11035
  11052
  11063
  11064
  11067
  11074
)

for subj in "${subjects[@]}"; do
  echo "Processing subject $subj"

  mrregister wmfod2/${subj}_wmfod.mif \
    -mask1 dwimask/${subj}_dwi_mask.mif \
    template3/wmfod_template_vox_2.mif \
    -nl_warp template3/maskintemp_2/${subj}_subject2template_warp.mif \
    template3/maskintemp_2/${subj}_template2subject_warp.mif \
    -force

  mrtransform dwimask/${subj}_dwi_mask.mif \
    -warp template3/maskintemp_2/${subj}_subject2template_warp.mif \
    -interp nearest -datatype bit \
    template3/maskintemp_2/${subj}_dwi_mask_in_template_space.mif \
    -force

done

mrmath template3/maskintemp/*_dwi_mask_in_template_space-10.mif min template3/template_mask_try3.mif -datatype bit


